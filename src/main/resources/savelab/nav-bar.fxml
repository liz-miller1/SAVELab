<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns:fx="http://javafx.com/fxml" fx:controller="savelab.NavBarController" spacing="20" styleClass="nav-bar">
    <ImageView fx:id="logoImage" fitHeight="50" fitWidth="50" onMouseClicked="#goToHome"/>
    <Button fx:id="homeButton" text="Home" onAction="#goToHome" styleClass="nav-button"/>
    <Button fx:id="researchButton" text="Research" onAction="#goToResearch" styleClass="nav-button"/>
    <Button fx:id="peopleButton" text="People" onAction="#goToPeople" styleClass="nav-button"/>
    <Button fx:id="locationButton" text="Location" onAction="#goToLocation" styleClass="nav-button"/>
</HBox>